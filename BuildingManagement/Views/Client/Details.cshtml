@model BuildingManagement.Models.Client

@{
    ViewBag.Title = BuildingManagement.Resources.Texts.Details;
}

<h2>@BuildingManagement.Resources.Texts.Details</h2>

<div>
    <h4>@BuildingManagement.Resources.Texts.Client</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>@BuildingManagement.Resources.Texts.Name</dt>
        <dd>@Html.DisplayFor(model => model.Name)</dd>
        <dt>@BuildingManagement.Resources.Texts.Phone</dt>
        <dd>@Html.DisplayFor(model => model.Phone)</dd>
        <dt>@BuildingManagement.Resources.Texts.Address</dt>
        <dd>@Html.DisplayFor(model => model.Address)</dd>
        <dt>@BuildingManagement.Resources.Texts.Contact</dt>
        <dd>@Html.DisplayFor(model => model.Contact)</dd>
        <dt>@BuildingManagement.Resources.Texts.Email</dt>
        <dd>@Html.DisplayFor(model => model.Email)</dd>
    </dl>
</div>
<p>
    @Html.ActionLink(BuildingManagement.Resources.Texts.Edit, "Edit", new { id = Model.ID }) 
    @Html.ActionLink(BuildingManagement.Resources.Texts.BackToList, "Index")
</p>

<script src="~/Scripts/jquery-3.3.1.js"></script>
